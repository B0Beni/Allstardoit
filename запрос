--SELECT album.Title, artist.Name FROM album INNER JOIN artist ON artist.ArtistId = album.AlbumId
--выбрали и обьединили по принципу соответствия
SELECT Track.Name as TrackName,
Genre.Name as GenreName,
Album.Title as AlbumTitle,
Artist.Name
FROM Track
LEFT JOIN Genre ON Track.GenreId = Genre.GenreId
LEFT JOIN Album ON Track.AlbumID = Album.AlbumID
LEFT JOIN Artist ON Album.ArtistID = Artist.ArtistID
--WHERE Artist.Name = "U2"  /*только для выбранной группы ю2*/
ORDER BY TrackName